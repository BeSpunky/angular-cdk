{
  "version": 1,
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "defaultProject": "official-site",
  "schematics": {
    "@nrwl/angular": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      },
      "storybook-configuration": {
        "linter": "eslint"
      }
    },
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress",
      "strict": true
    },
    "@nrwl/angular:library": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "strict": true
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
  },
  "projects": {
    "angular-cdk": {
      "projectType": "library",
      "root": "libs/angular-cdk",
      "sourceRoot": "libs/angular-cdk",
      "prefix": "bs",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:package",
          "outputs": ["dist/libs/angular-cdk"],
          "options": {
            "project": "libs/angular-cdk/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/angular-cdk/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "libs/angular-cdk/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/angular-cdk"],
          "options": {
            "jestConfig": "libs/angular-cdk/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/angular-cdk/**/*.ts",
              "libs/angular-cdk/**/*.html"
            ]
          }
        }
      }
    },
    "navigables": {
      "projectType": "library",
      "root": "libs/navigables",
      "sourceRoot": "libs/navigables",
      "prefix": "bs",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:package",
          "options": {
            "tsConfig": "libs/navigables/tsconfig.lib.json",
            "project": "libs/navigables/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/navigables/tsconfig.lib.prod.json"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/navigables/**/*.ts",
              "libs/navigables/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/navigables"],
          "options": {
            "jestConfig": "libs/navigables/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "official-site": {
      "projectType": "application",
      "root": "apps/official-site",
      "sourceRoot": "apps/official-site/src",
      "prefix": "angular-cdk",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/official-site",
            "index": "apps/official-site/src/index.html",
            "main": "apps/official-site/src/main.ts",
            "polyfills": "apps/official-site/src/polyfills.ts",
            "tsConfig": "apps/official-site/tsconfig.app.json",
            "assets": [
              "apps/official-site/src/favicon.ico",
              "apps/official-site/src/assets"
            ],
            "styles": ["apps/official-site/src/styles.scss"],
            "scripts": [],
            "vendorChunk": true,
            "extractLicenses": false,
            "buildOptimizer": false,
            "sourceMap": true,
            "optimization": false,
            "namedChunks": true
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/official-site/src/environments/environment.ts",
                  "with": "apps/official-site/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "official-site:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "official-site:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "official-site:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/official-site/src/**/*.ts",
              "apps/official-site/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/official-site"],
          "options": {
            "jestConfig": "apps/official-site/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "official-site-e2e": {
      "root": "apps/official-site-e2e",
      "sourceRoot": "apps/official-site-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/official-site-e2e/cypress.json",
            "tsConfig": "apps/official-site-e2e/tsconfig.e2e.json",
            "devServerTarget": "official-site:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "official-site:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/official-site-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "reactive-input": {
      "projectType": "library",
      "root": "libs/reactive-input",
      "sourceRoot": "libs/reactive-input",
      "prefix": "bs",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:package",
          "options": {
            "tsConfig": "libs/reactive-input/tsconfig.lib.json",
            "project": "libs/reactive-input/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/reactive-input/tsconfig.lib.prod.json"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/reactive-input/**/*.ts",
              "libs/reactive-input/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/reactive-input"],
          "options": {
            "jestConfig": "libs/reactive-input/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "rxjs": {
      "root": "libs/rxjs",
      "sourceRoot": "libs/rxjs",
      "projectType": "library",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/rxjs/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/rxjs"],
          "options": {
            "jestConfig": "libs/rxjs/jest.config.js",
            "passWithNoTests": true
          }
        },
        "build": {
          "builder": "@nrwl/node:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/rxjs",
            "tsConfig": "libs/rxjs/tsconfig.lib.json",
            "packageJson": "libs/rxjs/package.json",
            "main": "libs/rxjs/index.ts",
            "assets": ["libs/rxjs/*.md"]
          }
        }
      }
    },
    "timeline": {
      "projectType": "library",
      "root": "libs/timeline",
      "sourceRoot": "libs/timeline",
      "prefix": "bs",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:package",
          "options": {
            "tsConfig": "libs/timeline/tsconfig.lib.json",
            "project": "libs/timeline/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/timeline/tsconfig.lib.prod.json"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/timeline/**/*.ts",
              "libs/timeline/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/timeline"],
          "options": {
            "jestConfig": "libs/timeline/jest.config.js",
            "passWithNoTests": true
          }
        },
        "storybook": {
          "builder": "@nrwl/storybook:storybook",
          "options": {
            "uiFramework": "@storybook/angular",
            "port": 4400,
            "config": {
              "configFolder": "libs/timeline/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        },
        "build-storybook": {
          "builder": "@nrwl/storybook:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "uiFramework": "@storybook/angular",
            "outputPath": "dist/storybook/timeline",
            "config": {
              "configFolder": "libs/timeline/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        }
      }
    },
    "typescript-utils": {
      "root": "libs/typescript-utils",
      "sourceRoot": "libs/typescript-utils/src",
      "projectType": "library",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/typescript-utils/**/*.ts"]
          }
        },
        "build": {
          "builder": "@nrwl/node:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/typescript-utils",
            "tsConfig": "libs/typescript-utils/tsconfig.lib.json",
            "packageJson": "libs/typescript-utils/package.json",
            "main": "libs/typescript-utils/src/index.ts",
            "assets": ["libs/typescript-utils/*.md"]
          }
        }
      }
    }
  }
}
